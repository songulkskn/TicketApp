@page
@model TicketApp.Pages.TicketFolder.OpenTicketModel
@{
}



<table border="4" width="80%">
    <thead>
        <tr>
            <th>Konu</th>
            <th>Açıklama</th>
            <th>Tarih</th>
            <th></th>
        </tr>
    </thead>
    <tbody>

        @for (int i = 0; i < Model.TicketInput.Count; i++)
        {

            <tr>
                <td >@Model.TicketInput[i].Subject</td> 
                <td >@Model.TicketInput[i].Description </td>
                <td >@Model.TicketInput[i].OpenDate.Date </td>
                <td>
                    <a asp-page="./SetDetailTech" asp-route-id="@Model.TicketInput[i].Id">Set Tech Detail</a>

                </td>

            </tr>



        }


    </tbody>
</table>
